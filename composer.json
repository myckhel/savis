{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/myckhel/laravel-paystack"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-gd": "^8.0",
        "facade/ignition": "^2.5",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.2",
        "http-interop/http-factory-guzzle": "1.0",
        "laravel/framework": "^8.16",
        "laravel/octane": "^1.0",
        "laravel/sanctum": "^2.8",
        "laravel/telescope": "^4.6",
        "laravel/tinker": "^2.5",
        "nunomaduro/collision": "^5.1",
        "spatie/laravel-medialibrary": "^9.4",
        "spatie/laravel-permission": "dev-master",
        "unicodeveloper/laravel-paystack": "dev-master"
    },
    "require-dev": {
        "fakerphp/faker": "^1.12",
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^9.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "files": [
            "app/Helpers/index.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "dev": "php artisan octane:start --workers=1 --task-workers=1 --watch"
    }
}
