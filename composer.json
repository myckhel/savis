{
  "name": "laravel/laravel",
  "type": "project",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/stojankukrika/laravel-paystack"
    }
  ],
  "require": {
    "php": "^8.0",
    "ext-gd": "^8.0",
    "ext-openswoole": "*",
    "fruitcake/laravel-cors": "^3.0",
    "guzzlehttp/guzzle": "^7.4",
    "http-interop/http-factory-guzzle": "1.0",
    "inertiajs/inertia-laravel": "^0.6.3",
    "innocenzi/laravel-vite": "^0.2.0-beta.24",
    "intervention/image": "^2.7",
    "james-heinrich/getid3": "^1.9",
    "laravel/framework": "^9.9",
    "laravel/horizon": "^5.9",
    "laravel/octane": "^1.2",
    "laravel/sanctum": "^2.15",
    "laravel/telescope": "^4.8",
    "laravel/tinker": "^2.7",
    "myckhel/laravel-chat-system": "^1.0@beta",
    "nunomaduro/collision": "^6.2",
    "predis/predis": "^1.1",
    "spatie/laravel-medialibrary": "^10.3",
    "spatie/laravel-permission": "^5.5",
    "tightenco/ziggy": "^1.4",
    "unicodeveloper/laravel-paystack": "dev-master"
  },
  "require-dev": {
    "fakerphp/faker": "^1.19",
    "mockery/mockery": "^1.4.2",
    "phpunit/phpunit": "^9.3",
    "spatie/laravel-ignition": "^1.0"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "autoload": {
    "files": [
      "app/Helpers/index.php"
    ],
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ],
    "dev": [
      "Composer\\Config::disableProcessTimeout",
      "php artisan octane:start --workers=1 --task-workers=1 --watch"
    ]
  }
}
